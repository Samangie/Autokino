        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="530">
    <Grid x:Name="inhalt">
        <Button x:Name="zeigeListe" Content="Listenansicht" HorizontalAlignment="Left" Margin="212,198,0,0" VerticalAlignment="Top" Width="75" Click="zeigeListe_Click"/>
