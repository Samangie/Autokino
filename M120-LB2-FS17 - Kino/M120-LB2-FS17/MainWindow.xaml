        <Button x:Name="addFilm" Content="Film hinzufügen" HorizontalAlignment="Left" Margin="212,149,0,0" VerticalAlignment="Top" Width="75" Click="addFilm_Click" RenderTransformOrigin="0.499,-1.406"/>
        <TextBox x:Name="tbfilm" HorizontalAlignment="Left" Height="23" Margin="190,121,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
    </Grid>

</Window>
